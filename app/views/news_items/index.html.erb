<h1>Listing news_items</h1>

<table>
  <thead>
    <tr>
      <th>Release date</th>
      <th>Vendor</th>
      <th>Headline</th>
      <th>Topic</th>
      <th>Keywords</th>
      <th>Web url</th>
      <th>Doc url</th>
      <th>Comment</th>
      <th>Rsssource</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @news_items.each do |news_item| %>
      <tr>
        <td><%= news_item.release_date %></td>
        <td><%= news_item.vendor %></td>
        <td><%= news_item.headline %></td>
        <td><%= news_item.topic %></td>
        <td><%= news_item.keywords %></td>
        <td><%= news_item.web_url %></td>
        <td><%= news_item.doc_url %></td>
        <td><%= news_item.comment %></td>
        <td><%= news_item.RssSource %></td>
        <td><%= link_to 'Show', news_item %></td>
        <td><%= link_to 'Edit', edit_news_item_path(news_item) %></td>
        <td><%= link_to 'Destroy', news_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New News item', new_news_item_path %>
